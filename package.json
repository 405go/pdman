{
  "name": "PDMan",
  "version": "2.2.0",
  "description": "Model design tool",
  "main": "build/main.js",
  "scripts": {
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron": "electron ./src/main",
    "start": "node bin/start.js",
    "build": "node bin/build.js",
    "package-win": "npm run build && build --win ",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "package-mac": "npm run build && build --mac"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/robergroup/pdman"
  },
  "homepage": "http://www.pdman.cn",
  "author": {
    "name": "robergroup",
    "email": "qliu@405go.cn"
  },
  "license": "ISC",
  "dependencies": {
    "@antv/g6": "^1.2.8",
    "antd": "^3.0.1",
    "brace": "^0.11.1",
    "dot": "^1.1.2",
    "hoist-non-react-statics": "^2.3.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "officegen": "^0.4.7",
    "pdfkit": "^0.8.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-ace": "^6.1.1",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "electron": "^3.0.0",
    "electron-builder": "^20.4.1",
    "electron-rebuild": "^1.7.3",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "react-dev-utils": "^4.2.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "xml-loader": "^1.2.1"
  },
  "build": {
    "appId": "com.pdman.robergroup",
    "productName": "PDMan",
    "artifactName": "${name}-${platform}_v${version}.${ext}",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "build/"
    ],
    "npmRebuild": false,
    "asar": true,
    "asarUnpack": [
      "build/jar/",
      "build/word/"
    ],
    "dmg": {
      "icon": "build/icon.icns",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "icon": "build/256x256.png",
      "target": [
        "zip",
        "deb",
        "AppImage"
      ]
    }
  }
}
